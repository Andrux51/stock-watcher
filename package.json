{
  "name": "meanjs-starter",
  "version": "0.0.1",
  "description": "A starter template for MEAN stack applications",
  "main": "server.js",
  "scripts": {
    "start": "nodemon server.js",
    "test": "mocha -R min test/api/**/*.js && karma start test/client/karma.conf.js && grunt mochacov",
    "testlocal": "mocha -R min test/api/**/*.js && karma start test/client/karma.conf.js"
  },
  "config": {
    "blanket": {
      "pattern": "test/api/**.*.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Andrux51/meanjs-starter"
  },
  "contributors": [
    "Andy Martin <andymartin51@gmail.com> (https://github.com/Andrux51)"
  ],
  "dependencies": {
    "async": "^0.9.2",
    "bower": "latest",
    "body-parser": "^1.15.0",
    "ejs": "^1.0.0",
    "express": "^4.13.3",
    "nodemailer": "^2.3.2"
  },
  "devDependencies": {
    "blanket": "^1.1.7",
    "chai": "^3.4.0",
    "coveralls": "^2.11.4",
    "grunt": "^0.4.5",
    "grunt-contrib-clean": "^0.6.0",
    "grunt-contrib-concat": "^0.5.1",
    "grunt-contrib-copy": "^0.6.0",
    "grunt-contrib-cssmin": "^0.11.0",
    "grunt-contrib-less": "^1.0.1",
    "grunt-contrib-uglify": "^0.7.0",
    "grunt-contrib-watch": "^0.6.1",
    "grunt-filerev": "^2.3.1",
    "grunt-karma": "^0.9.0",
    "grunt-mocha-cov": "^0.4.0",
    "grunt-usemin": "^3.1.1",
    "jasmine-core": "^2.3.4",
    "karma": "^0.12.37",
    "karma-coverage": "^0.2.7",
    "karma-jasmine": "^0.3.6",
    "karma-json-fixtures-preprocessor": "0.0.5",
    "karma-phantomjs-launcher": "^0.2.1",
    "less-plugin-autoprefix": "^1.5.1",
    "less-plugin-clean-css": "^1.5.1",
    "phantomjs": "^1.9.18"
  }
}
